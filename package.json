{
  "name": "aoitelegram",
  "description": "A powerful package to create Telegram Bots with built-in string functions",
  "main": "./dist/index.js",
  "typing": "./dist/index.d.ts",
  "version": "1.0.1",
  "scripts": {
    "clean:dist": "rm -rf dist/",
    "uglifyjs": "find ./dist/function -type f -name \"*.d.ts\" -exec rm {} && uglifyjs --in-situ dist/function/**/**",
    "build:test": "npm run clean:dist && tsc && tsc-alias",
    "build:release": "npm run clean:dist && tsc && tsc-alias && npm run uglifyjs"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "engines": {
    "node": ">=16"
  },
  "license": "MIT",
  "files": [
    "./dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "author": "Sempai<zadmanthegreat@gmail.com>",
  "homepage": "https://aoitelegram.vercel.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sempai-07/aoitelegram.git"
  },
  "bugs": {
    "url": "https://github.com/Sempai-07/aoitelegram/issues"
  },
  "keywords": [
    "javascript",
    "typescript",
    "telegram",
    "library",
    "telegramapi",
    "bot",
    "aoi",
    "aoitelegram",
    "bds.js",
    "@aoitelegram",
    "aoi.js"
  ],
  "dependencies": {
    "chalk": "^4.1.2",
    "figlet": "^1.7.0",
    "long-timeout": "^0.1.1",
    "loupe": "^3.1.0",
    "ms": "^2.1.3",
    "node-fetch": "^2.7.0",
    "telegramsjs": "^3.3.0",
    "tslib": "^2.6.2",
    "@aoitelegram/util": "^1.0.1"
  },
  "devDependencies": {
    "@telegram.ts/types": "^1.12.0",
    "@types/figlet": "^1.5.8",
    "@types/long-timeout": "^0.1.2",
    "@types/ms": "^0.7.34",
    "@types/node": "^18.19.26",
    "@types/node-fetch": "^2.6.11",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.5.3"
  }
}
