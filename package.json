{
  "name": "aoitelegram",
  "description": "A powerful package to create Telegram Bots with built-in string functions",
  "main": "./dist/src/index.js",
  "typing": "./dist/src/index.d.ts",
  "version": "0.8.2",
  "scripts": {
    "clean:dist": "rm -rf dist/",
    "uglifyjs": "find ./dist/src/function -name \"*.js\" -type f -print0 | xargs -0 uglifyjs --in-situ",
    "build:release": "npm run clean:dist && npm run build:tsc:main && npm run build:tsc:function && npm run uglifyjs",
    "build:tsc:main": "tsc --project tsconfig.json",
    "build:tsc:function": "tsc --project ./src/function/tsconfig.json"
  },
  "engines": {
    "node": ">=16"
  },
  "license": "MIT",
  "files": [
    "./dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "author": "Sempai<zadmanthegreat@gmail.com>",
  "homepage": "https://aoitelegram.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sempai-07/aoitelegram"
  },
  "bugs": {
    "url": "https://github.com/Sempai-07/aoitelegram/issues"
  },
  "keywords": [
    "javascript",
    "typescript",
    "telegram",
    "library",
    "telegramapi",
    "bot",
    "aoi",
    "aoitelegram",
    "bds.js",
    "@aoitelegram",
    "aoi.js"
  ],
  "dependencies": {
    "@aoitelegram/database": "^0.2.1",
    "chalk": "^4.1.2",
    "figlet": "^1.7.0",
    "fs-extra": "^11.2.0",
    "import-sync": "^2.2.0",
    "long-timeout": "^0.1.1",
    "loupe": "^3.1.0",
    "ms": "^2.1.3",
    "node-fetch": "^2.7.0",
    "telegramsjs": "^3.0.0-beta.24",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@telegram.ts/types": "^1.8.0",
    "@types/figlet": "^1.5.8",
    "@types/fs-extra": "^11.0.4",
    "@types/is-regex": "^1.0.2",
    "@types/long-timeout": "^0.1.2",
    "@types/ms": "^0.7.34",
    "@types/node": "^18.19.26",
    "@types/node-fetch": "^2.6.11"
  }
}
